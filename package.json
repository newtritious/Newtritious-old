  "bugs": {
    "url": "https://github.com/ichbindev/alumni-collab/issues"
  },
  "homepage": "https://github.com/ichbindev/alumni-collab#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
